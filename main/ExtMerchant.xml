<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <!-- 加载lua文件 -->
    <Script file="ExtMerchant.lua"/>

    <Frame name="ExtMerchant">
        <Scripts>
            <OnLoad>
                <!-- 初始化加载例程 -->
                ExtMerchant_OnLoad(self)
            </OnLoad>
            <OnEvent>
                <!-- 事件处理程序 -->
                ExtMerchant_OnEvent(self, event, ...)
            </OnEvent>
            <OnUpdate>
                <!-- 更新处理程序 -->
                ExtMerchant_OnUpdate(self, elapsed)
            </OnUpdate>
        </Scripts>
    </Frame>
    
</Ui>
